    "cross-env": "^7.0.3",
    "electron": "^11.3.0",
    "electron-packager": "^15.2.0",
    "node-sass": "^4.14.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.2",
    "typescript-plugin-css-modules": "^3.0.1"
