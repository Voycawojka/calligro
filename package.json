      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "client-zip": "^1.2.2",
    "helpful-decorators": "^2.1.0",
    "js-cookie": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-github-corner": "^2.5.0",
    "react-router": "^5.2.0",
